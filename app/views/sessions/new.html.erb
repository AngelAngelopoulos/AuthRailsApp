<div class="login">
  <div class="container">
    <h1>Log in</h1>
      <%= form_for(:session, url: login_path, :authenticity_token => false) do |f| %>  
    <div class="field"> 
      <%= f.label 'Email' %><br>
      <%= f.text_field :email %>
      <%= f.label 'Password' %>
      <%= f.password_field :password %>
    </div> 
    <div class="actions"> 
      <%= f.submit "Create" %> 
    </div> 
  <% end %> 

    
  </div>
</div>